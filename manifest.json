{
	"manifest_version": 2,

	"name": "ChromeScript",
	"description": "This extension makes writing scripts easy.",
	"version": "0.1",
	"default_locale": "en",
	"icons": {},
	"browser-action": {
		"default_icon": "",
		"default_title": "ChromeScript",
		"default_popup": "chromescript.html"
	},
	"author": "Mayank Patke",
	"background": {

	},
	"incognito": "split",
	"offline_enabled": "true",

	"permissions": [
		"<all_urls>",
		"contentSettings",
		"contextMenus",
		"cookies",
		"tabs"
	]
}