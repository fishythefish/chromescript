{
	"manifest_version": 2,

	"name": "ChromeScript",
	"description": "This extension makes writing scripts easy.",
	"version": "0.1",
	"default_locale": "en",
	"icons": {
		"16": "icon/icon16.png",
		"48": "icon/icon48.png",
		"128": "icon/icon128.png"
	},
	"browser-action": {
		"default_icon": {
			"19": "icon/icon19.png",
			"38": "icon/icon38.png"
		},
		"default_title": "ChromeScript",
		"default_popup": "chromescript.html"
	},
	"author": "Mayank Patke",
	"background": {

	},
	"incognito": "split",
	"offline_enabled": "true",

	"permissions": [
		"<all_urls>",
		"contentSettings",
		"contextMenus",
		"cookies",
		"tabs"
	]
}